<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke android:color="#b3b327" android:width="10dp" />
            <solid android:color="#f0f046"/>
            <size android:width="250dp" android:height="250dp"/>
        </shape>
    </item>
    <item android:top="70dp" android:bottom="150dp" android:left="60dp" android:right="160dp">
        <shape android:shape="oval">
            <solid android:color="#b3b327"/>
        </shape>
    </item>
    <item android:top="70dp" android:bottom="150dp" android:left="160dp" android:right="60dp">
        <shape android:shape="oval">
            <solid android:color="#b3b327"/>
        </shape>
    </item>
    <item android:top="150dp" android:bottom="80dp" android:left="60dp" android:right="60dp">
        <shape android:shape="rectangle">
            <solid android:color="#b3b327"/>
            <corners
                android:topLeftRadius="20dp"
                android:topRightRadius="20dp"
                android:bottomLeftRadius="20dp"
                android:bottomRightRadius="20dp"/>
        </shape>
    </item>
</layer-list>