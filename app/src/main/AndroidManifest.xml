<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jwernikowski.pam_lab">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.activity.rhythm_details.RhythmDetailsActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ui.activity.song_practice.SongPracticeActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ui.activity.song_details.SongDetailsActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ui.activity.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>